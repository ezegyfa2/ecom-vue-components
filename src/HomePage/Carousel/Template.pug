section.carousel
    #heroControls.carousel.slide(data-ride='carousel')
        ol.carousel-indicators
            li.active(data-target='#heroControls' data-slide-to='0')
            li(data-target='#heroControls' data-slide-to='1')
            li(data-target='#heroControls' data-slide-to='2')
        .carousel-inner
            .carousel-item.active
                img.img-fluid(src='@/assets/jum1.jpg' alt='First slide')
            .carousel-item
                img.img-fluid(src='@/assets/jum2.jpg' alt='Second slide')
            .carousel-item
                img.img-fluid(src='@/assets/jum3.jpg' alt='Third slide')
